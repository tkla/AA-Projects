<h1>Sign Up</h1>
<form action="<%= users_url %>" method="POST" >

<label for="Username" >Username</label>
<input id="Username" name="user[username]" value="<%=@user.username %>" type="text">

<label for="Email" >Email</label>
<input id="Email" name="user[email]" value="<%=@user.email %>" type="text">

<label for="Password" >Password</label>
<input id="Password" name="user[password]" value="<%= %>" type="password">

<input type="submit" value="Create User" >

</form> 